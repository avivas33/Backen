# Archivo de ejemplo para variables de entorno
# Copia este archivo a .env y configura tus valores

# Google reCAPTCHA
GOOGLE_RECAPTCHA_PROJECT_ID=tu-project-id
GOOGLE_RECAPTCHA_API_KEY=tu-api-key
GOOGLE_RECAPTCHA_SITE_KEY=tu-site-key

# Resend Email Service
RESEND_API_KEY=tu-resend-api-key
RESEND_FROM_EMAIL=noreply@tudominio.com
RESEND_FROM_NAME=Tu Empresa

# Hansa API Configuration
HANSA_BASE_URL=https://api.hansa.com
HANSA_COMPANY_CODE=TU_CODIGO
HANSA_USUARIO=tu-usuario
HANSA_CLAVE=tu-clave

# Payment Providers
COBALT_CLIENT_ID=tu-cobalt-client-id
COBALT_CLIENT_SECRET=tu-cobalt-client-secret
YAPPY_MERCHANT_ID=tu-yappy-merchant-id
PAYPAL_CLIENT_ID=tu-paypal-client-id
PAYPAL_CLIENT_SECRET=tu-paypal-client-secret
PAYPAL_MODE=sandbox

# CORS Origins (separados por comas)
ALLOWED_ORIGINS=https://selfservice.celero.network,https://selfservice-dev.celero.network,http://localhost:8080